{% load media %}{% load photos %}<?xml version="1.0" encoding="utf-8"?>
<playlist>
    <collection>
        <id>{{ media.slug }}</id>
        <title>{{ media.get_title }}</title>
        <description>{{ media.get_description|striptags }}</description>
        <videoHomePageTitle>{{ media.get_title }}</videoHomePageTitle>
        <videoHomePageURL>{{ media.get_domain_url }}</videoHomePageURL>
        <videoPages>
        {% for usage in media.get_usage %}
            <item>
                <title>{{ usage.title }}</title>
                <url>{{ usage.url }}</url>
            </item>
        {% endfor %}
        </videoPages>
        <relatedVideos>
        {% related 10 instruction.instruction for media.get_home as related_list %}
        {% for related in related_list %}
            <item>
                <title>{{ related.get_title }}</title>
                <description>{{ related.get_description|striptags }}</description>
                {% img base for media.get_photo as thumb %}
                <img>{{ thumb.url }}</img>
                <url>{{ related.get_domain_url }}</url>
            </item>
        {% endfor %}
        </relatedVideos>

        {% media mobile-video for media as formatted %}
        <mobileVideoLink>{{ formatted }}</mobileVideoLink>


        <placeToWebHTML><![CDATA[<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://fpdownload.macromedia.com/get/flashplayer/current/swflash.cab" name="VP" width="437" height="480" id="VP"><param name="movie" value="main.swf" /><param name="quality" value="high" /><param name="bgcolor" value="#000000" /><param name="allowScriptAccess" value="sameDomain" /><embed src="main.swf" quality="high" bgcolor="#000000" width="437" height="480" name="VP" align="middle" play="true"  loop="false" quality="high" allowScriptAccess="sameDomain" type="application/x-shockwave-flash" pluginspage="http://www.adobe.com/go/getflashplayer"></embed></object></noscript>]]></placeToWebHTML>
        <getLink>{{ media.get_domain_url }}</getLink>

        {% img base for media.get_photo as thumb %}
        <preview>{{ thumb.url }}</preview>

        {% media flash-video for media as formatted %}
        <items>
            <item>
                <path>{{ formatted }} </path>
                <fileStream>false</fileStream>
                <duration>100</duration>
            </item>
        </items>

        <bookmarks>
            <bookmark title="linkuj.cz" url="http://linkuj.cz/?id=linkuj&amp;url={{ media.get_domain_url|urlencode }}&amp;title={{ media.get_title|urlencode }}&amp;imgsrc=&amp;description={{ media.get_description|urlencode }}" />
            <bookmark title="bomba.cz" url="http://www.bomba.cz/video/pridat/?url={{ media.get_domain_url|urlencode }}" />
            <bookmark title="del.icio.us" url="http://del.icio.us/post?url={{ media.get_domain_url|urlencode }}&amp;title={{ media.get_title|urlencode }}" />
            <bookmark title="digg.com" url="http://digg.com/submit?phase=2&amp;url={{ media.get_domain_url|urlencode }}" />

        </bookmarks>

        <chapters>
            {% for section in media.get_sections %}
            <chapter>
                <time>{{ section.time.seconds }}</time>
                <title>{{ section.title }}</title>
            </chapter>
            {% endfor %}
        </chapters>


    </collection>


    <messages>
        <message name="continuePlayInFullscreen">Pokračuj v přehrávání ve fullscreen</message>
        <message name="continuePlay">Pokračuj v přehrávání</message>


        <message name="downloadTitle">Stáhnout video</message>
        <message name="downloadVideo">Stáhnout video v mobilním formátu do PC</message>
        <message name="downloadButton">Stáhnout</message>
        <message name="sendVideo">Zaslat video na mobilní telefon</message>
        <message name="phoneNumber">Tel. číslo</message>
        <message name="phoneNumberInput">+420777123456</message>
        <message name="sendVideoButton">Odeslat</message>
        <message name="sendToPhoneInfo">Při zaslání videa na mobilní telefon platíte za příchozí SMS 5,- a za data stahování videosouboru do mobilního telefonu.</message>

        <message name="moreInfoNextPagesTitle">Stránky s tímto videem</message>
        <message name="moreInfoNext">další</message>


        <message name="shareVideoTitle">Sdílet video</message>
        <message name="sendEmail">Poslat na email:</message>
        <message name="sendEmailButton">Odeslat</message>
        <message name="placeToWeb">Umístit na web:</message>
        <message name="placeToWebButton">Kopírovat</message>
        <message name="getLink">Získat odkaz:</message>
        <message name="getLinkButton">Kopírovat</message>
        <message name="internetShare">Sdílet na internetu</message>

        <message name="relatedTitle">Související videa</message>
    </messages>

    <settings>
        <autoplay>true</autoplay>
        <iframeURL><![CDATA[/static/jaknato/swf/player/iframe.html]]></iframeURL>
        <sendmailURL>{{ sendmail_url }}</sendmailURL>
    </settings>

    <adverts>
        <advert name="videoBefore"><![CDATA[oMKQwcwvN7ZucY04Fo7CJQtI7EGjTFCcyqtcMg2IzrL.K7]]></advert>
        <advert name="text" disappearDelay="10"><![CDATA[uVHq3JG8490YVoN6.WqfrXNkYMmq_2MSNNltj4nXCur.a7]]></advert>
        <advert name="relatedVideoBanner"><![CDATA[pCDxW52vDIpNgxvSxKOKeRW27mEtUptG8oRrXcyUpwv.37]]></advert>
    </adverts>

</playlist>
