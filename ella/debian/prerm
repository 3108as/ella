#!/bin/bash

set -e

# remove symlink for original django templates
ELLA_ADMIN_PATH='/usr/share/python-support/centrum-ella/ella/ellaadmin'
INST_DIR='/var/lib/python-support/python2.4/ella/'
for l in $ELLA_ADMIN_PATH/templates/admin_orig $INST_DIR/ellaadmin/templates/admin_orig; do
	[[ -L $i ]] && unlink $i
done

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

